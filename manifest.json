{
    "manifest_version": 2,
    "name": "Youtube Timeline",
    "version": "0.1",

    "description": "Soundcloud comments for YouTube",

    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "css": ["timeline.css"],
            "js": ["jquery.js", "timeline.js"]
        }
    ]
}